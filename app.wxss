/*dialog css for scaning successfully*/

.ui-cover-layer, .ui-dialog {
  display: none;
}

.ui-cover-layer.active {
  display: block;
  position: fixed;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  background-color: black;
  opacity: 0.5;
  z-index: 50;
}

.ui-dialog.active {
  display: block;
  position: fixed;
  bottom: 0rpx;
  z-index: 100;
  width: 100%;
  background-color: #fff;
  border-radius: 8rpx;
}

.ui-dialog .ui-header {
  font-size: 34rpx;
  margin-top: 60rpx;
  margin-bottom: 30rpx;
  text-align: center;
  color: #333;
}

.ui-dialog .ui-content {
  font-size: 30rpx;
  text-align: center;
  margin: 0 30rpx;
}

.ui-dialog .ui-content .price {
  margin-top: 20rpx;
  margin-bottom: 30rpx;
  color: #999;
}

.ui-dialog .ui-footer {
  font-size: 32rpx;
  /*margin:50rpx 20rpx 0 20rpx;*/
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 30rpx;
}

.ui-dialog .ui-footer view {
  flex: 1;
  /*display: inline-block;
    width:100%;*/
  text-align: center;
  line-height: 80rpx;
  height: 80rpx;
  margin: 50rpx 30rpx 0rpx 15rpx;
  background-color: #fede5b;
  border-radius: 8rpx;
  font-size: 32rpx;
  color: #333;
}

.ui-dialog .ui-footer .cancel {
  border: 2rpx solid #efefef;
  background-color: #fff;
  margin-right: 15rpx;
  margin-left: 30rpx;
}

.ui-dialog .calculate {
  flex: 5;
  height: 54rpx;
}

.ui-dialog .calculate .sum {
  float: right;
  margin-right: 20rpx;
  color: #f95d5b;
}

.ui-dialog .calculate .inline {
  display: inline-block;
  width: 52rpx;
  height: 54rpx;
  text-align: center;
  line-height: 54rpx;
  vertical-align: middle;
  border: 1px solid #ccc;
}

.ui-dialog .calculate .inline.minus {
  border-top-left-radius: 5rpx;
  border-bottom-left-radius: 5rpx;
}

/* .ui-dialog .calculate .minus.disable{
    background-color:#efefef;
} */

.ui-dialog .calculate .inline.minus:after {
  content: '\e75b';
  font-size: 24rpx;
}

.ui-dialog .calculate .inline.minus.disable:after {
  color: #ccc;
}

.ui-dialog .calculate .inline.add {
  border-top-right-radius: 5rpx;
  border-bottom-right-radius: 5rpx;
}

/* .ui-dialog .calculate .add.disable{
    background-color:#efefef;
} */

.ui-dialog .calculate .inline.add:after {
  content: '\e75c';
  font-size: 24rpx;
}

.ui-dialog .calculate .inline.add.disable:after {
  color: #ccc;
}

.ui-dialog .calculate .number {
  width: 74rpx;
  border-left-width: 0px;
  border-right-width: 0px;
}
